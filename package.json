{
  "name": "sim-element",
  "version": "0.0.3",
  "description": "Reactive, component-oriented view layer for modern web interfaces.",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist",
    "src/lib",
    "src/utils"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:docs": "vite build --config vite.docs.config.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^2.7.3",
    "vue-router": "3.5.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue2": "^1.1.2",
    "element-ui": "2.15.7",
    "escape-html": "^1.0.3",
    "github-markdown-css": "^5.1.0",
    "highlight.js": "^11.5.1",
    "markdown-it-container": "^3.0.0",
    "prismjs": "^1.28.0",
    "sass": "^1.53.0",
    "vite": "^2.8.0",
    "vite-plugin-md": "^0.18.1",
    "vite-plugin-vue2": "^2.0.1",
    "vue-template-compiler": "^2.7.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "babel-loader"
      ],
      "allowedVersions": {
        "@angular/common": "13"
      }
    }
  }
}